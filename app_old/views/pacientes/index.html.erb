<h1>Listing pacientes</h1>

<table>
  <tr>
    <th>Cpf</th>
    <th>Rg</th>
    <th>Nome</th>
    <th>Cep</th>
    <th>Endereco</th>
    <th>Numero</th>
    <th>Bairro</th>
    <th>Cidade</th>
    <th>Uf</th>
    <th>Complemento</th>
    <th>Sexo</th>
    <th>Estado civil</th>
    <th>Data nascimento</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pacientes.each do |paciente| %>
  <tr>
    <td><%= paciente.cpf %></td>
    <td><%= paciente.rg %></td>
    <td><%= paciente.nome %></td>
    <td><%= paciente.cep %></td>
    <td><%= paciente.endereco %></td>
    <td><%= paciente.numero %></td>
    <td><%= paciente.bairro %></td>
    <td><%= paciente.cidade %></td>
    <td><%= paciente.uf %></td>
    <td><%= paciente.complemento %></td>
    <td><%= paciente.sexo %></td>
    <td><%= paciente.estado_Civil %></td>
    <td><%= paciente.data_nascimento %></td>
    <td><%= link_to 'Show', paciente %></td>
    <td><%= link_to 'Edit', edit_paciente_path(paciente) %></td>
    <td><%= link_to 'Destroy', paciente, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Paciente', new_paciente_path %>
